# Environment Configuration for LLM SQL Agent

# LLM API Keys (choose your provider)
# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini (alternative to OpenAI)
GOOGLE_API_KEY=your_google_api_key_here

# Database Configuration
DATABASE_PATH=./database/sample.db
QUERY_TIMEOUT=30
MAX_RESULTS=100

# Agent Configuration
LLM_PROVIDER=openai  # Options: openai, google
MODEL_NAME=gpt-4     # For OpenAI: gpt-4, gpt-3.5-turbo | For Google: gemini-1.5-pro
TEMPERATURE=0.1
MAX_TOKENS=2000

# Debugging
DEBUG_MODE=false
LOG_LEVEL=INFO
